{
  "name": "ionic-ng-pwa",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "ionic-build": "rimraf www && ionic build",
    "ionic-aot": "npm run ionic-build -- --aot",
    "ionic-start": "npm run ionic-build -- --watch",
    "android-studio": "npm run ionic-aot && npx cap sync android && npx cap open android",
    "android-device": "ionic capacitor run android -l --external --host=localhost --port=4200 --livereload-port=4200",
    "android-live": "npm-run-all -p ionic-start android-device",
    "ios-xcode": "npm run ionic-aot && npx cap sync ios && npx cap open ios",
    "ios-device": "ionic capacitor run ios -l --external --host=localhost --port=4200 --livereload-port=4200",
    "ios-live": "npm-run-all -p ionic-start ios-device",
    "ionic-build-prod": "npm run ionic-build -- --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pre-commit": "lint-staged",
    "format": "prettier --write 'src/**/*.{js,ts,json,html,css,scss,md}'",
    "prepare": "is-ci || husky install"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "@angular/service-worker": "~13.0.0",
    "@capacitor/android": "3.4.0",
    "@capacitor/app": "1.1.0",
    "@capacitor/camera": "^1.2.4",
    "@capacitor/core": "3.4.0",
    "@capacitor/filesystem": "^1.1.0",
    "@capacitor/haptics": "1.1.4",
    "@capacitor/ios": "^3.4.0",
    "@capacitor/keyboard": "1.2.1",
    "@capacitor/status-bar": "1.0.7",
    "@capacitor/storage": "^1.2.4",
    "@ionic/angular": "^6.0.0",
    "@ionic/pwa-elements": "^3.1.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.1",
    "@angular-eslint/builder": "~13.0.1",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "~13.0.1",
    "@angular/compiler": "~13.0.0",
    "@angular/compiler-cli": "~13.0.0",
    "@angular/language-service": "~13.0.0",
    "@capacitor/cli": "3.4.0",
    "@ionic/angular-toolkit": "^5.0.0",
    "@ionic/cli": "^6.18.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^12.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.4.4"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ],
    "*.{json,css,scss,html,md}": [
      "pretty-quick --staged"
    ]
  },
  "description": "An Ionic PWA project"
}
